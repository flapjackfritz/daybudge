{
  "scripts": {
    "start": "NODE_PATH=./lib PORT=8080 pm2 start lib/server.js --watch --interpreter babel-node --name appDev",
    "stop": "pm2 kill",
    "log": "pm2 log",
    "webpack": "webpack -wd",
    "build-webpack": "webpack -p",
    "build-node": "babel lib -d build --copy-files",
    "start-prod": "NODE_ENV=production NODE_PATH=./build pm2 start build/server.js -i max --name appProd"
  },
  "babel": {
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "name": "daybudge",
  "version": "1.0.0",
  "description": "Day to day finance tracking",
  "main": "lib/server.js",
  "author": "Tommy Fritz",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.3.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "flux": "^3.1.3",
    "lodash.throttle": "^4.1.1",
    "lodash.union": "^4.6.0",
    "node-uuid": "^1.4.8",
    "pm2": "^2.8.0",
    "react": "^16.2.0",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "webpack": "^3.8.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "eslint": "^4.13.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-react": "^7.5.1",
    "redux-devtools": "^3.4.1"
  }
}
